{
  "name": "tailadmin-react",
  "private": true,
  "version": "2.1.2",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"vite\" \"npm:backend\"",
  "backend": "bash -lc 'ROOT=$(pwd); while [ \"$ROOT\" != \"/\" ]; do if [ -f \"$ROOT/Lacajita/lApi.py\" ] || [ -f \"$ROOT/Lacajita/Api.py\" ]; then break; fi; ROOT=$(dirname \"$ROOT\"); done; if [ -f \"$ROOT/Lacajita/lApi.py\" ]; then (command -v curl >/dev/null 2>&1 && curl -s --max-time 2 http://127.0.0.1:8001/health >/dev/null 2>&1 && echo \"Using existing API service at http://127.0.0.1:8001\" && sleep infinity) || (cd \"$ROOT/Lacajita\" && . venv/bin/activate && PORT=8001 exec python3 lApi.py); elif [ -f \"$ROOT/Lacajita/Api.py\" ]; then (cd \"$ROOT/Lacajita\" && . venv/bin/activate && PORT=8001 exec python3 Api.py); else echo \"Lacajita API not found in parent dirs\"; exit 1; fi'",
  "build": "NODE_OPTIONS=--max-old-space-size=4096 vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "start-prod": "serve -s dist -l 5174",
  "serve-with-api": "bash ./scripts/serve_with_api.sh"
  },
  "dependencies": {
    "@auth0/auth0-react": "^2.3.0",
    "@fullcalendar/core": "^6.1.15",
    "@fullcalendar/daygrid": "^6.1.15",
    "@fullcalendar/interaction": "^6.1.15",
    "@fullcalendar/list": "^6.1.15",
    "@fullcalendar/react": "^6.1.15",
    "@fullcalendar/timegrid": "^6.1.15",
    "@popperjs/core": "^2.11.8",
    "@react-jvectormap/core": "^1.0.4",
    "@react-jvectormap/world": "^1.1.2",
    "@sentry/react": "^9.39.0",
    "@sentry/vite-plugin": "^3.6.0",
    "@tailwindcss/forms": "^0.5.10",
    "apexcharts": "^4.1.0",
    "flatpickr": "^4.6.13",
    "react": "^19.0.0",
    "react-apexcharts": "^1.7.0",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "react-dom": "^19.0.0",
    "react-dropzone": "^14.3.5",
    "react-helmet-async": "^2.0.5",
    "react-router": "^7.5.2",
    "react-router-dom": "^7.1.5",
    "react-secure-storage": "^1.3.2",
    "simplebar-react": "^3.3.0",
    "swiper": "^11.2.3",
    "tailwind-merge": "^3.0.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.19.0",
    "@tailwindcss/postcss": "^4.0.0",
    "@types/node": "^20.19.11",
    "@types/react": "^19.0.12",
    "@types/react-dom": "^19.0.4",
    "@vitejs/plugin-react": "^4.3.4",
    "concurrently": "^8.2.0",
    "eslint": "^9.19.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.18",
    "globals": "^15.14.0",
    "postcss": "^8.5.2",
  "serve": "^14.1.2",
    "tailwindcss": "^4.0.0",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.22.0",
  "vite": "^6.3.6",
    "vite-plugin-svgr": "^4.3.0"
  },
  "overrides": {
    "react-helmet-async": {
      "react": "^16.8.0 || ^17 || ^18 || ^19"
    },
    "@react-jvectormap/core": {
      "react": "^16.8.0 || ^17 || ^18 || ^19",
      "react-dom": "^16.8.0 || ^17 || ^18 || ^19"
    },
    "@react-jvectormap/world": {
      "react": "^16.8.0 || ^17 || ^18 || ^19",
      "react-dom": "^16.8.0 || ^17 || ^18 || ^19"
    }
  }
}
